#include <iostream>

using namespace std;

int main() {
	bool divisibilidad_por_5;
	int num;

	cout << "Ingrese un numero:" << endl;
	cin >> num;

	divisibilidad_por_5 = (num % 5 == 0);

	if (divisibilidad_por_5) {
		cout << "Es divisible por 5";
	}
	else {
		cout << "No es divisible por 5";
	}

	return 0;
}
