#include <iostream>

using namespace std;

int main () {
    int numeros[5];
    int sum = 0;
    
    cout << "Ingrese 5 numeros:" << endl;
    
    for (int i = 0; i < 5; i++){
        cin >> numeros[i];
        sum += numeros[i];
    }
    cout << "La suma de numeros es: " << sum;
    return 0;
}
